{
  "name": "nuxt-apollo-client",
  "version": "1.0.2",
  "author": "safdar-azeem",
  "description": "Nuxt module for Apollo Client with SSR and codegen support.",
  "repository": "https://github.com/safdar-azeem/nuxt-apollo-client",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "nuxt",
    "apollo-client",
    "nuxt-ssr",
    "apollo-ssr",
    "apollo-codegen",
    "graphql-codegen",
    "nuxt-codegen",
    "apollo",
    "graphql",
    "vue",
    "vue-apollo",
    "vue-apollo-composable",
    "codegen",
    "codegen-client",
    "vue-codegen",
    "nuxt-module"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "node scripts/build-wrapper.js",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "postinstall": "node scripts/install-peer-deps.js",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.10.8",
    "@nuxt/kit": "^3.12.3",
    "@vue/apollo-composable": "^4.0.2",
    "@vue/apollo-option": "^4.0.0",
    "apollo-upload-client": "^18.0.1",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "js-cookie": "^3.0.5"
  },
  "peerDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.3.2",
    "@graphql-codegen/typescript-vue-apollo": "^4.1.1",
    "vite-plugin-graphql-codegen": "^3.3.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript-vue-apollo": "^4.1.1",
    "@nuxt/devtools": "^1.3.9",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/module-builder": "^0.8.1",
    "@nuxt/schema": "^3.12.3",
    "@nuxt/test-utils": "^3.13.1",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@parcel/watcher": "^2.4.1",
    "@types/apollo-upload-client": "^18.0.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "changelogen": "^0.5.5",
    "eslint": "^9.6.0",
    "glob": "^9.3.5",
    "nuxt": "^3.12.3",
    "rollup": "^4.18.1",
    "typescript": "latest",
    "vite-plugin-graphql-codegen": "^3.3.8",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.24"
  },
  "resolutions": {
    "glob": "^9.0.0",
    "rollup": "^4.18.1",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "endOfLine": "auto",
    "bracketSameLine": true
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}